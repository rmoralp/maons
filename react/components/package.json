{
  "name": "@maons/react-components",
  "version": "1.1.5",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/tailwind.css -o ./.storybook/tailwind.css",
    "build:storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "build": "concurrently \"npm:build:*\"",
    "compile": "tsc",
    "check": "tsc --noEmit",
    "deploy": "npx chromatic --build-script-name build --exit-zero-on-changes",
    "dev": "concurrently \"npm:watch:*\"",
    "prepare": "rm -rf dist && npm run compile",
    "lint": "eslint --ext tsx,ts src",
    "test": "vitest",
    "test:ci": "vitest --run",
    "watch:css": "npx tailwindcss -i ./src/tailwind.css -o ./.storybook/tailwind.css --watch",
    "watch:storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@maons/react-hooks": "*",
    "@radix-ui/react-dialog": "1.0.2",
    "@radix-ui/react-toast": "1.1.2",
    "clsx": "1.2.1",
    "tailwind-merge": "1.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@maons/lint": "*",
    "@maons/tsconfig": "*",
    "@mdx-js/react": "2.2.1",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack4": "6.5.16",
    "@storybook/manager-webpack4": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.1.2",
    "chromatic": "6.15.0",
    "concurrently": "7.6.0",
    "jsdom": "21.0.0",
    "postcss": "8.4.21",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.5",
    "vitest": "0.28.3"
  },
  "eslintConfig": {
    "extends": "../../node_modules/@maons/lint/.eslintrc.js"
  }
}
